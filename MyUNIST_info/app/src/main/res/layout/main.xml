<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/mainlayout"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:baselineAligned="false"
    android:background="@drawable/mainbg">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="10px"
        android:layout_weight="1"
        android:id="@+id/unist_intro">

        <Button
            android:id="@+id/auth_btn"
            android:layout_width="50dp"
            android:layout_height="70dp"
            android:layout_marginRight="30px"
            android:layout_marginTop="40dp"
            android:background="@drawable/macot"
            android:layout_alignParentLeft="false"
            android:layout_marginLeft="638dp"
            android:layout_alignParentTop="false"
            android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Welcome to"
            android:id="@+id/welcome"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="50dp"
            android:textSize="18dp"
            android:layout_alignParentEnd="false"
            android:elegantTextHeight="false" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="U N I S T"
            android:id="@+id/textView"
            android:layout_below="@+id/welcome"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="8dp"
            android:textSize="40dp"
            android:textStyle="bold"
            android:elegantTextHeight="false" />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="10px"
        android:layout_weight="1"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="10px"
            android:layout_weight="1"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="10px"
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/c11"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c12"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c13"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c14"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c15"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="10px"
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/c21"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c22"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c23"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c24"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <Button
                        android:id="@+id/b24"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/dhe"
                        android:elevation="5dp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c25"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <Button
                        android:id="@+id/b25"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/eche"
                        android:elevation="5dp" />
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="10px"
            android:layout_weight="1"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="10px"
                android:layout_weight="1"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/c31"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c32"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c33"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <Button
                        android:id="@+id/b33"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/ece"
                        android:elevation="5dp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c34"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <Button
                        android:id="@+id/b34"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/ls"
                        android:elevation="5dp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c35"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <Button
                        android:id="@+id/b35"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/mne"
                        android:elevation="5dp" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="10px"
                android:layout_weight="1"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/c41"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c42"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <Button
                        android:id="@+id/b42"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/mse"
                        android:elevation="5dp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c43"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <Button
                        android:id="@+id/b43"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/ba"
                        android:elevation="5dp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c44"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <Button
                        android:id="@+id/b44"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/sns"
                        android:elevation="5dp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/c45"
                    android:layout_width="10px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <Button
                        android:id="@+id/b45"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/uee"
                        android:elegantTextHeight="false"
                        android:elevation="5dp" />
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>


</LinearLayout>
