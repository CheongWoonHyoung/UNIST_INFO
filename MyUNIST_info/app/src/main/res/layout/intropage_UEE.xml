<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="0dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="#FFFFFF"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/backside">

            <ImageView
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/ba_icon"
                android:background="@drawable/uee"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="40dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="70dp"
                android:backgroundTint="#FFFFFF"
                android:elevation="10dp" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="120dp"
                android:layout_marginBottom="30dp"
                android:orientation="vertical"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:id="@+id/relativeLayout">

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="230dp"
                android:orientation="vertical"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/relativeLayout"
                android:id="@+id/Upper"
                android:layout_alignParentEnd="false">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text ="환경과학공학"
                    android:textSize="25dp"
                    android:background="#00b1b7"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="0dp"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="0dp"
                    android:padding="5dp"
                    android:textColor="#FFFFFF"
                    android:id="@+id/textView3"
                    android:elevation="10dp" />
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:text ="지구환경"
                    android:id="@+id/btn1"
                    android:layout_below="@+id/textView3"
                    android:layout_marginTop="15dp"
                    android:textColor="#FFFFFF"
                    android:layout_marginLeft="30dp"
                    android:background="#FDB933"
                    android:elevation="10dp"
                    android:padding="10dp"
                    android:textAlignment="center" />
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:text ="수처리공학"
                    android:id="@+id/btn2"
                    android:textColor="#FFFFFF"
                    android:elevation="10dp"
                    android:background="#FDB933"
                    android:layout_alignTop="@+id/btn1"
                    android:layout_centerHorizontal="true"
                    android:padding="10dp" />
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:text ="기후변화공학"
                    android:id="@+id/btn3"
                    android:textColor="#FFFFFF"
                    android:background="#FDB933"
                    android:elevation="10dp"
                    android:padding="10dp"
                    android:layout_alignTop="@+id/btn2"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginRight="30dp"
                    android:layout_marginEnd="30dp" />
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:text ="환경공학개론"
                    android:id="@+id/btn4"
                    android:layout_below="@+id/btn3"
                    android:layout_marginLeft="30dp"
                    android:textColor="#FFFFFF"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="15dp"
                    android:background="#FDB933"
                    android:elevation="10dp"
                    android:padding="10dp" />
                TextView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:text ="생산관리"
                android:id="@+id/btn5"
                android:textColor="#FFFFFF"
                android:layout_alignTop="@+id/btn4"
                android:layout_alignLeft="@+id/btn2"
                android:layout_alignStart="@+id/btn2"
                android:padding="10dp"
                android:background="#FDB933"
                android:elevation="10dp" />
                TextView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:text ="경영전략"
                android:id="@+id/btn6"
                android:textColor="#FFFFFF"
                android:background="#FDB933"
                android:elevation="10dp"
                android:layout_alignTop="@+id/btn5"
                android:layout_alignLeft="@+id/btn3"
                android:layout_alignStart="@+id/btn3"
                android:padding="10dp" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:orientation="vertical"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/Upper"
                android:id="@+id/Lower">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text ="도시건설공학"
                    android:textSize="25dp"
                    android:background="#00b1b7"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="0dp"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="0dp"
                    android:padding="5dp"
                    android:textColor="#FFFFFF"
                    android:id="@+id/textView4"
                    android:elevation="10dp" />
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:text ="도시계획개론"
                    android:id="@+id/btn5"
                    android:layout_below="@+id/textView4"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="30dp"
                    android:textColor="#FFFFFF"
                    android:background="#FDB933"
                    android:elevation="10dp"
                    android:padding="10dp" />
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:text ="건설공학개론"
                    android:id="@+id/btn6"
                    android:textColor="#FFFFFF"
                    android:background="#FDB933"
                    android:elevation="10dp"
                    android:layout_alignTop="@+id/btn5"
                    android:layout_centerHorizontal="true"
                    android:padding="10dp" />
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:text ="지리정보시스템"
                    android:id="@+id/btn7"
                    android:textColor="#FFFFFF"
                    android:background="#FDB933"
                    android:elevation="10dp"
                    android:layout_alignTop="@+id/btn6"
                    android:elegantTextHeight="false"
                    android:padding="10dp"
                    android:layout_marginRight="30dp"
                    android:layout_alignParentEnd="true" /><![CDATA[
                TextView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:text ="미시경제학"
                android:id="@+id/btn11"
                android:textColor="#FFFFFF"
                android:background="#FDB933"
                android:elevation="10dp"
                android:layout_alignTop="@+id/btn10"
                android:layout_alignLeft="@+id/btn8"
                android:layout_alignStart="@+id/btn8"
                android:padding="10dp" />
                TextView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:text ="투자론"
                android:id="@+id/btn12"
                android:textColor="#FFFFFF"
                android:background="#FDB933"
                android:elevation="10dp"
                android:layout_alignTop="@+id/btn11"
                android:layout_alignLeft="@+id/btn9"
                android:layout_alignStart="@+id/btn9"
                android:padding="10dp" />
            ]]>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_alignParentLeft="true"
                android:layout_alignParentBottom="true"
                android:layout_below="@+id/Lower">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text ="재난관리공학"
                    android:textSize="25dp"
                    android:background="#00b1b7"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="0dp"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="0dp"
                    android:padding="5dp"
                    android:textColor="#FFFFFF"
                    android:id="@+id/textView5"
                    android:elevation="10dp" />
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:text ="자연재해개론"
                    android:id="@+id/btn8"
                    android:layout_below="@+id/textView5"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="30dp"
                    android:textColor="#FFFFFF"
                    android:background="#FDB933"
                    android:elevation="10dp"
                    android:padding="10dp" />
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:text ="재난관리"
                    android:id="@+id/btn9"
                    android:textColor="#FFFFFF"
                    android:background="#FDB933"
                    android:elevation="10dp"
                    android:layout_alignTop="@+id/btn8"
                    android:layout_centerHorizontal="true"
                    android:padding="10dp" />
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:text ="재9해분석과 시스템의 안정성"
                    android:id="@+id/btn10"
                    android:layout_marginLeft="43dp"
                    android:textColor="#FFFFFF"
                    android:background="#FDB933"
                    android:elevation="10dp"
                    android:layout_alignTop="@+id/btn9"
                    android:elegantTextHeight="false"
                    android:padding="10dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginRight="30dp" />
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:text ="날씨분석 및 예측"
                    android:id="@+id/btn12"
                    android:layout_below="@+id/btn8"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginLeft="30dp"
                    android:textColor="#FFFFFF"
                    android:background="#FDB933"
                    android:elevation="10dp"
                    android:padding="10dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginBottom="100dp" /><![CDATA[
                TextView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:text ="투자론"
                android:id="@+id/btn12"
                android:textColor="#FFFFFF"
                android:background="#0a0857"
                android:elevation="10dp"
                android:layout_alignTop="@+id/btn11"
                android:layout_alignLeft="@+id/btn9"
                android:layout_alignStart="@+id/btn9"
                android:padding="10dp" />
            ]]>
            </RelativeLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="주요 개설 교과목"
                android:textSize="30dp"
                android:textColor="#FFFFFF"
                android:layout_alignTop="@+id/ba_icon"
                android:layout_toRightOf="@+id/ba_icon"
                android:layout_toEndOf="@+id/ba_icon"
                android:layout_marginLeft="20dp"
                android:background="#0a0857"
                android:elevation="10dp"
                android:textAlignment="center"
                android:paddingLeft="15dp"
                android:paddingTop="5dp" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/imageView2"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:adjustViewBounds="false"
                android:layout_above="@+id/Upper" />
        </RelativeLayout>
    </ScrollView>

</RelativeLayout>
