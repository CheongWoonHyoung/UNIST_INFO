<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="0dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp">

    </ScrollView>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="#FFFFFF"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/backside">

        <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:id="@+id/imageView"
            android:background="@drawable/ba"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="40dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="70dp"
            android:backgroundTint="#000000" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="120dp"
            android:layout_marginBottom="30dp"
            android:orientation="vertical"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:id="@+id/relativeLayout">

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="260dp"
            android:orientation="vertical"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/relativeLayout"
            android:id="@+id/relativeLayout2"
            android:layout_alignParentEnd="false">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text ="경영학"
                android:textSize="25dp"
                android:background="#00b1b7"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="0dp"
                android:padding="5dp"
                android:textColor="#FFFFFF"
                android:id="@+id/textView3" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text ="조직행동론"
                android:id="@+id/btn1"
                android:layout_below="@+id/textView3"
                android:layout_marginTop="10dp"
                android:textColor="#000000"
                android:layout_marginLeft="30dp"
                android:paddingBottom="18dp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text ="마케팅 관리"
                android:paddingBottom="18dp"
                android:id="@+id/btn2"
                android:layout_below="@+id/btn1"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="30dp"
                android:textColor="#000000" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text ="재무회계"
                android:paddingBottom="18dp"
                android:id="@+id/btn3"
                android:layout_below="@+id/btn2"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="30dp"
                android:textColor="#000000"
                android:layout_alignParentStart="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text ="재무관리"
                android:paddingBottom="18dp"
                android:id="@+id/btn4"
                android:layout_below="@+id/btn3"
                android:layout_marginLeft="30dp"
                android:textColor="#000000"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text ="생산관리"
                android:paddingBottom="18dp"
                android:id="@+id/btn5"
                android:layout_below="@+id/btn4"
                android:layout_marginLeft="30dp"
                android:textColor="#000000"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text ="경영전략"
                android:paddingBottom="18dp"
                android:id="@+id/btn6"
                android:layout_below="@+id/btn5"
                android:layout_marginLeft="30dp"
                android:textColor="#000000"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:layout_below="@+id/relativeLayout2">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text ="재무/회계"
                android:textSize="25dp"
                android:background="#00b1b7"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="0dp"
                android:padding="5dp"
                android:textColor="#FFFFFF"
                android:id="@+id/textView4" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text ="재무회계"
                android:id="@+id/btn7"
                android:layout_below="@+id/textView4"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="30dp"
                android:paddingBottom="18dp"
                android:textColor="#000000" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text ="관리회계"
                android:paddingBottom="18dp"
                android:id="@+id/btn8"
                android:layout_below="@+id/btn7"
                android:layout_alignLeft="@+id/btn7"
                android:layout_alignStart="@+id/btn7"
                android:textColor="#000000" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text ="재무관리"
                android:id="@+id/btn9"
                android:layout_below="@+id/btn8"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="30dp"
                android:paddingBottom="18dp"
                android:textColor="#000000" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text ="경영통계 분석"
                android:id="@+id/btn10"
                android:layout_below="@+id/btn9"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="30dp"
                android:paddingBottom="18dp"
                android:textColor="#000000" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text ="미시경제학"
                android:id="@+id/btn11"
                android:layout_below="@+id/btn10"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="30dp"
                android:paddingBottom="18dp"
                android:textColor="#000000" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text ="투자론"
                android:id="@+id/btn12"
                android:layout_below="@+id/btn11"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="30dp"
                android:paddingBottom="18dp"
                android:textColor="#000000" />
        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="주요 개설 교과목"
            android:textSize="30dp"
            android:textColor="#FFFFFF"
            android:layout_alignTop="@+id/imageView"
            android:layout_toRightOf="@+id/imageView"
            android:layout_toEndOf="@+id/imageView"
            android:layout_marginLeft="20dp"
            android:padding="5dp"
            android:background="#0a0857" />
    </RelativeLayout>
</RelativeLayout>
